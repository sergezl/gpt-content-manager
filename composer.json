{
    "name": "sergezlobovski/gpt-content-manager",
    "description": "WordPress Plugin for Managing Website Content with ChatGPT",
    "type": "wordpress-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Serge Zlobovski",
            "email": "serge.zlobovski@gmail.com",
            "homepage": "https://github.com/sergezlobovski"
        }
    ],
    "autoload": {
        "psr-4": {
            "GptContentManager\\": "src/"
        }
    },
    "require": {
        "php": ">=8.0",
        "openai-php/client": "^0.8.4",
        "symfony/http-client": "^7.0",
        "nyholm/psr7": "^1.8",
        "guzzlehttp/guzzle": "^7.8"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
